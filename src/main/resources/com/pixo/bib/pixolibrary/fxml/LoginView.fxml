<Pane fx:id="mainPane" prefHeight="568.0" prefWidth="902.0" stylesheets="@../CSS/loginView.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pixo.bib.pixolibrary.Controllers.LoginController">
        <children>
            <TextField fx:id="email" layoutX="301.0" layoutY="198.0" prefHeight="38.0" prefWidth="318.0" promptText="exemple : toto@gmail.com" />
            <Button fx:id="goRegister" layoutX="397.0" layoutY="458.0" onAction="#register" prefHeight="26.0" prefWidth="109.0" text="Register" />
            <Button fx:id="loginButton" layoutX="401.0" layoutY="297.0" mnemonicParsing="false" onAction="#login" prefHeight="32.0" prefWidth="101.0" text="LoginButton" />
      <Label fx:id="messageLabel" layoutX="335.0" layoutY="430.0" prefHeight="19.0" prefWidth="250.0" text="you don't have an Account !!" textFill="#f5f3f3">
